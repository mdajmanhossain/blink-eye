{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "ReminderWindow"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-close",
    "window:allow-create",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "webview:allow-create-webview-window",
    "webview:allow-webview-close",
    "shell:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:default",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled"
  ]
}